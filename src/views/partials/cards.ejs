<% carro.forEach((obj) => { %>
<div class="card" style="width: 18rem">
  <img src="../img/carro.jpg" class="card-img-top" alt="..." />
  <div class="card-body">
    <h5 class="card-title"><%= obj.veiculo %></h5>
    <p class="card-text">Descrição: <%= obj.descricao %></p>
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">Ano: <%= obj.ano %></li>
    <li class="list-group-item">Vendido: <%= obj.vendido %></li>
    <li class="list-group-item">Ult. Edição: <%= obj.updated %></li>
  </ul>
  <div class="card-body">
    <a href="/vender/<%= obj.id %>" class="card-link classa">Vender</a>
    <a href="/editar/<%= obj.id %>" class="card-link classa">Editar</a>
  </div>
</div>
<% }) %>
